MODULE user_command_2001 INPUT.
CASE SY-UCOMM.

  WHEN 'HOME'.
  LEAVE PROGRAM.

  WHEN 'EXECUTE'.
  PERFORM READ_HU.


  WHEN 'CREATE'.
  PERFORM CREATE_HU_WT.



ENDCASE.
ENDMODULE.
